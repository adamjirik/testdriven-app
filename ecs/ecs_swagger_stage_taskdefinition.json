{
  "containerDefinitions" : [
    {
      "name": "swagger",
      "image": "%s.dkr.ecr.eu-central-1.amazonaws.com/test-driven-swagger:staging",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "URL",
          "value": "swagger.json"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "testdriven-swagger-stage",
          "awslogs-region": "eu-central-1"
        }
      }
    },
  ],
  "family": "testdriven-swagger-stage-td"
}